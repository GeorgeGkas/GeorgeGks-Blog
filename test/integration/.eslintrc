{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "plugins": ["import"],
  "env": {
    "mocha": true,
    "node": true,
    "jquery": true,
    "browser": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "global-require": 0
  }
}
